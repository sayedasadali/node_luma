<html>
	<head>
		<title>Lab Status</title>
		<link href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet">
		<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.10/angular.min.js"></script>
		<script src="http://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.10/angular-ui-router.js"></script>
		<script src="/javascripts/angularApp.js"></script>
		<style> .glyphicon-thumbs-up { cursor:pointer } </style>
	</head>

	<!-- <body ng-app="labz" ng-controller='MainCtrl'> -->
	<body ng-app="labz">
	<div class = 'container'>
		<div class="row col-md-6 col-md-offset-3">
				<div class="page-header">
					<h1>Lab Status</h1>
				</div>

				<div>
				<ui-view></ui-view>
				<script type="text/ng-template" id="/home.html">
				<div ng-repeat="comp in computers">
					<span class="glyphicon glyphicon-info-sign"></span>
					{{comp.cOS}}

					<span class="glyphicon glyphicon-hdd"></span>
					<span ng-class="{'text-success': comp.cState === 'OPEN', 'text-warning' : comp.cState === 'IN USE'}">{{comp.cState}}</span>
					<span style="font-size:15px; margin-left:10px;">
					{{comp.cName}}
					</span>

				</div>
				</script>

			</div>
		</div>
	</div>
	</body>
</html>